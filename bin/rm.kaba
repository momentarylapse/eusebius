use lib.std
use lib.std.file


func _rm(filename: string)
	if unlink(filename)
		return
	if lib_error == -ENOENT
		print("rm: {{filename}} does not exist")
	else if lib_error == -EISDIR
		print("rm: {{filename}} is a directory")
	else
		print("rm: can not delete file: {{filename}}")
	exit(lib_error)


func main()
	init_lib()
	let args = get_args()
	
	for a in args
		_rm(a)
	
	exit(0)
