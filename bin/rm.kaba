#define __OS__
use "lib/std.kaba"


void _rm(string filename)
	int r = unlink(filename)
	if r == 0
		return
	if r == -ENOENT
		print("rm: " + filename + " existiert nicht\n")
	else if r == -EISDIR
		print("rm: " + filename + " ist ein Verzeichnis\n")
	else
		print("rm: kann Datei nicht loeschen: " + filename + "\n")
	exit(r)


void main()
	init_lib()
	string[] arg
	get_args(arg)
	
	for i in 1:arg.num
		_rm(arg[i])
	
	exit(0)
