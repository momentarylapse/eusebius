use "lib/std.kaba"


void _rm(string filename)
	if unlink(filename)
		return
	if lib_error == -ENOENT
		print("rm: " + filename + " does not exist")
	else if lib_error == -EISDIR
		print("rm: " + filename + " is a directory")
	else
		print("rm: can not delete file: " + filename)
	exit(lib_error)


void main()
	init_lib()
	string[] arg
	get_args(arg)
	
	for i in 1:arg.num
		_rm(arg[i])
	
	exit(0)
