#define __OS__
use "prog_header.kaba"



void main()
	init_lib()
	
	SysInfo s
	if sysinfo(s) == 0
		print "uptime: "
		print s.uptime
		print "\n"
		print "free: "
		print (s.freeram * s.mem_unit) / 1024
		print "k  "
		print "total: "
		print (s.totalram * s.mem_unit) / 1024
		print "k  "
		print "shared: "
		print (s.sharedram * s.mem_unit) / 1024
		print "k\n"
	
	showstats()
	
	exit(0)
