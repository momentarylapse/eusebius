use "lib/std.kaba"

class UTSName
	cstring sysname
	cstring nodename
	cstring release
	cstring version
	cstring machine
	cstring domainname
	void __assign__(UTSName o)
		pass

void main()
	init_lib()
	lib_print_endl = false
	
	UTSName u
	if uname(&u)
		print(cs2s(u.sysname))
		print("  ")
		print(cs2s(u.nodename))
		print("  ")
		print(cs2s(u.release))
		print("  ")
		print(cs2s(u.version))
		print("  ")
		print(cs2s(u.machine))
		print("\n")
	
	exit(0)
