use "lib/std.kaba"

class UTSName
	cstring sysname
	cstring nodename
	cstring release
	cstring version
	cstring machine
	cstring domainname
	void __assign__(UTSName o)
		pass

void main()
	init_lib()
	
	UTSName u
	if !uname(&u)
		print("ERROR")
		exit(1)
	print("{{cs2s(u.sysname)}}  {{cs2s(u.nodename)}}  {{cs2s(u.release)}}  {{cs2s(u.version)}}  {{cs2s(u.machine)}}")
	
	exit(0)
