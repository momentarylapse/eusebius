use lib.std
use lib.std.sys

class UTSName
	var sysname: cstring
	var nodename: cstring
	var release: cstring
	var version: cstring
	var machine: cstring
	var domainname: cstring

func main()
	init_lib()
	
	var u: UTSName
	if not uname(&u)
		print("ERROR")
		exit(1)
	print("{{cs2s(u.sysname)}}  {{cs2s(u.nodename)}}  {{cs2s(u.release)}}  {{cs2s(u.version)}}  {{cs2s(u.machine)}}")
	
	exit(0)
