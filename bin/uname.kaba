#define __OS__
use "lib/std.kaba"

class UTSName
	cstring sysname
	cstring nodename
	cstring release
	cstring version
	cstring machine
	cstring domainname

void main()
	init_lib()
	
	UTSName u
	if uname(&u) == 0
		print cs2s(u.sysname)
		print "  "
		print cs2s(u.nodename)
		print "  "
		print cs2s(u.release)
		print "  "
		print cs2s(u.version)
		print "  "
		print cs2s(u.machine)
		print "\n"
	
	exit(0)
