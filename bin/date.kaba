use "lib/std.kaba"



void main()
	init_lib()
	lib_print_endl = false
	
	TimeValue t
	if !time(t)
		exit(lib_error)
	print(t.sec / 3600)
	print(":")
	print((t.sec / 600) % 6)
	print((t.sec / 60) % 10)
	print(":")
	print((t.sec / 10) % 6)
	print((t.sec     ) % 10)
	print(" ")
	print(t.nanosec)
	print("ns\n")
	
	exit(0)
