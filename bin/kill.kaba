use lib.std

func main()
	init_lib()
	let args = get_args()
	
	var signal = SIGKILL
	var pid: int[]
	
	if len(args) > 0
		for a in args
			let n = int(a)
			if n < 0
				signal = -n
			else
				pid.add(n)
		for p in pid
			if !kill(p, signal)
				print("kill: process {{p}} not found")
				exit(-1)
	else
		print("kill: kill [-SIGNAL] <PID>")
	exit(0)
