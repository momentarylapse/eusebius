int _p2i(void* p)
	int* i = &p
	return *i

class PointerWrapper
	void *p

void *_i2p(int i)
	PointerWrapper *p = &i
	return p.p


class Word
	char[2] c
	int _int()
		int a = c[0]
		int b = c[1]
		return a + b * 256
	void set(int i)
		c[0] = i
		c[1] = i >> 8
	void __iadd__(int d)
		set(_int() + d)
		


int call_ppii(void *f, void *a, void *b, int c, int d)
	__temp_p__ = f
	__temp_p2__ = a
	__temp_p3__ = b
	__temp_i__ = c
	__temp_i2__ = d
	asm{
		mov eax, __temp_p__
		push __temp_i2__
		push __temp_i__
		push __temp_p3__
		push __temp_p2__
		call eax
		mov __temp_i__, eax
		sub esp, 0x10
	}
	return __temp_i__


