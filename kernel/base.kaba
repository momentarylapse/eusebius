int strlen(void* _s)
	cstring* s = _s
	for i in 0:256
		if s[i] == 0
			return i
	return 256

void strcpy(void* _a, void* _b)
	cstring* a = _a
	cstring* b = _b
	for i in 0:256
		a[i] = b[i]
		if b[i] == 0
			break

void strcat(void* _a, void* _b)
	cstring* a = _a
	cstring* b = _b
	int n = strlen(a)
	strcpy(&a[n], b)

int strcmp(void* _a, void* _b)
	cstring* a = _a
	cstring* b = _b
	for i in 0:256
		if a[i] > b[i]
			return 1
		else if a[i] < b[i]
			return -1
		if a[i] == 0
			break
	return 0



int ceil_quot(int n, int m)
	return (n + m - 1) / m

