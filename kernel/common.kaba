func strlen(s: cstring) -> int
	for i in 0:256
		if s[i] == 0
			return i
	return 256

func strcpy(out a: cstring, b: cstring)
	for i in 0:256
		a[i] = b[i]
		if b[i] == 0
			break

func strcat(out a: cstring, b: cstring)
	let n = strlen(a)
	for i in 0:256
		a[i+n] = b[i]
		if b[i] == 0
			break

func strcmp(a: cstring, b: cstring) -> int
	for i in 0:256
		if a[i] > b[i]
			return 1
		else if a[i] < b[i]
			return -1
		if a[i] == 0
			break
	return 0



func ceil_quot(n: int, m: int) -> int
	return (n + m - 1) / m

