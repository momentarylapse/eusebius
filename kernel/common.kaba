func _p2i(p: void*) -> int
	var i: int* = &p
	return *i
func _p2i64(p: void*) -> int64
	var i: int64* = &p
	return *i

class PointerWrapper
	var p: void*

func _i2p(i: int) -> void*
	return _i642p(i)

func _i642p(i: int64) -> void*
	var p: PointerWrapper* = &i
	return p.p


class Word
	var c: char[2]
	func _int() -> int
		let a: int = c[0]
		let b: int = c[1]
		return a + b * 256
	func mut set(i: int)
		c[0] = i
		c[1] = i >> 8
	func mut __iadd__(d: int)
		set(_int() + d)
		


